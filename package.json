{
  "name": "moonbloom-theme",
  "displayName": "Moonbloom Official Theme",
  "description": "Gentle on the eyes, evoking the cozy tranquility of a serene night.",
  "publisher": "Moonbloom",
  "version": "1.0.2",
  "scripts": {
    "convert-colors": "node ./scripts/convert-color-hex-to-uppercase.cjs ./color-schema.js && node ./scripts/convert-color-hex-to-uppercase.cjs ./themes/moonbloom.json",
    "vsce-publish": "vsce publish",
    "ovsx-publish": "ovsx publish",
    "prepublish": "npm run convert-colors",
    "publish": "npm run vsce-publish && npm run ovsx-publish"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Moonbloom Official Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/moonbloom.json"
      }
    ]
  },
  "license": "MIT",
  "maintainers": [
    "Igor Teplostanski"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moonbloom-theme/visual-studio-code.git"
  },
  "bugs": {
    "url": "https://github.com/moonbloom-theme/visual-studio-code/issues"
  },
  "keywords": [
    "moonbloom",
    "dark",
    "theme",
    "color-theme"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1D1E27",
    "theme": "dark"
  },
  "__metadata": {
    "size": 92613
  },
  "devDependencies": {
    "ovsx": "^0.10.1",
    "vsce": "^2.15.0"
  }
}
